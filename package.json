{
  "name": "@dinhoabreu/semantic-release-bitbucket-config",
  "description": "semantic-release shareable config to publish npm packages with Bitbucket",
  "version": "0.0.0-development",
  "author": "Edison E. Abreu (https://github.com/dinhoabreu)",
  "bugs": {
    "url": "https://github.com/dinhoabreu/semantic-release-bitbucket-config/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "~7.1.2",
    "@semantic-release/npm": "~7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.2"
  },
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "semantic-release": "^17.3.0",
    "xo": "^0.39.1"
  },
  "engines": {
    "node": ">=10.18"
  },
  "files": [
    "index.js",
    "templates"
  ],
  "homepage": "https://github.com/dinhoabreu/semantic-release-bitbucket-config#readme",
  "keywords": [
    "bitbucket",
    "conventional-changelog",
    "release",
    "semantic-release",
    "semantic-release-config",
    "version"
  ],
  "license": "MIT",
  "main": "index.js",
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "semi": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dinhoabreu/semantic-release-bitbucket-config.git"
  },
  "scripts": {
    "cm": "git-cz",
    "semantic-release": "semantic-release",
    "test": "xo"
  },
  "xo": {
    "prettier": true,
    "space": true
  }
}
