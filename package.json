{
  "name": "@dinhoabreu/semantic-release-bitbucket-config",
  "description": "semantic-release shareable config to publish npm packages with Bitbucket",
  "version": "0.0.0-development",
  "author": "Edison E. Abreu (https://github.com/dinhoabreu)",
  "bugs": {
    "url": "https://github.com/dinhoabreu/semantic-release-bitbucket-config/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/commit-analyzer": "^6.3.3",
    "@semantic-release/git": "^7.0.18",
    "@semantic-release/npm": "^5.3.5",
    "@semantic-release/release-notes-generator": "^7.3.5"
  },
  "devDependencies": {
    "commitizen": "^3.1.2",
    "cz-conventional-changelog": "^2.0.0",
    "semantic-release": "^17.2.3",
    "xo": "~0.25.0"
  },
  "engines": {
    "node": ">=8.3"
  },
  "files": [
    "index.js",
    "templates"
  ],
  "homepage": "https://github.com/dinhoabreu/semantic-release-bitbucket-config#readme",
  "keywords": [
    "bitbucket",
    "conventional-changelog",
    "release",
    "semantic-release",
    "semantic-release-config",
    "version"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "semantic-release": ">=15.10.0 <16.0.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "semi": false
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dinhoabreu/semantic-release-bitbucket-config.git"
  },
  "scripts": {
    "cm": "git-cz",
    "semantic-release": "semantic-release",
    "test": "xo"
  },
  "xo": {
    "prettier": true,
    "space": true
  }
}
